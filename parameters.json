{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "dockerRegistryImageName": {
            "value": "appsvc/apps:wordpress"
        },
        "databaseName": {
            "value": "wordpress"
        },
        "administratorLogin": {
            "value": "mysqldbuser"
        },
